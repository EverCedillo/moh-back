        <class>mx.ohanahome.app.backend.model.Payee</class>
        <class>mx.ohanahome.app.backend.model.Permission</class>
        <class>mx.ohanahome.app.backend.model.PurchaseLimit</class>
        <class>mx.ohanahome.app.backend.model.Role</class>
        <class>mx.ohanahome.app.backend.model.Scales</class>
